{
  "mcpServers": {
    "amap-maps": {
      "url": "https://mcp.amap.com/mcp?key=99e97af6fd426ce3cfc45d22d26e78e3"
    },
    "google-maps": {
      "command": "npx",
      "args": ["-y", "@google/google-maps-mcp-server"],
      "env": {
        "GOOGLE_MAPS_API_KEY": "YOUR_GOOGLE_MAPS_API_KEY"
      }
    },
    "yelp": {
      "command": "npx",
      "args": ["-y", "@yelp/yelp-mcp-server"],
      "env": {
        "YELP_API_KEY": "YOUR_YELP_API_KEY"
      }
    },
    "tripadvisor": {
      "command": "npx",
      "args": ["-y", "@tripadvisor/tripadvisor-mcp-server"],
      "env": {
        "TRIPADVISOR_API_KEY": "YOUR_TRIPADVISOR_API_KEY"
      }
    },
    "jinko-hotel": {
      "command": "npx",
      "args": ["-y", "@jinko/hotel-booking-mcp-server"],
      "env": {
        "JINKO_API_KEY": "YOUR_JINKO_API_KEY"
      }
    },
    "airbnb": {
      "command": "npx",
      "args": ["-y", "@openbnb/mcp-server-airbnb"]
    },
    "amadeus-flight": {
      "command": "npx",
      "args": ["-y", "@amadeus/flight-search-mcp-server"],
      "env": {
        "AMADEUS_API_KEY": "YOUR_AMADEUS_API_KEY",
        "AMADEUS_API_SECRET": "YOUR_AMADEUS_API_SECRET"
      }
    },
    "openweathermap": {
      "command": "npx",
      "args": ["-y", "@openweathermap/openweathermap-mcp-server"],
      "env": {
        "OPENWEATHER_API_KEY": "YOUR_OPENWEATHER_API_KEY"
      }
    }
  }
}
